<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ApiBundle\Entity\LogActivity" table="log_activity">
    <indexes>
      <index name="fk_log_activity_list_idx" columns="list_id"/>
      <index name="fk_log_activity_task_idx" columns="task_id"/>
      <index name="fk_log_activity_type_activity_idx" columns="type_activity_id"/>
      <index name="fk_log_activity_user_idx" columns="user_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="datetimeActivity" type="datetime" column="datetime_activity" nullable="false"/>
    <many-to-one field="user" target-entity="FosUser" fetch="LAZY">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="typeActivity" target-entity="TypeActivity" fetch="LAZY">
      <join-columns>
        <join-column name="type_activity_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="task" target-entity="Task" fetch="LAZY">
      <join-columns>
        <join-column name="task_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="list" target-entity="Tasklist" fetch="LAZY">
      <join-columns>
        <join-column name="list_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
